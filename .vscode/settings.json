{
    "cSpell.words": [
        "cakey",
        "camelcase",
        "fulluri",
        "hexy",
        "ipsec",
        "jsrsasign",
        "KEYUTIL",
        "KJUR",
        "makedoc",
        "nodenext",
        "notafter",
        "notbefore",
        "prvpem",
        "pubpem",
        "sbjpubkey",
        "secp",
        "sigalg",
        "sshpk",
        "strparse",
        "Struct"
    ],
    "git.ignoreLimitWarning": true,
    "editor.codeActionsOnSave": {
        "source.biome": "always",
        "source.fixAll.biome": "explicit",
        "source.organizeImports.biome": "explicit",
        "source.removeUnusedImports": "always"
    },
    "editor.defaultFormatter": "biomejs.biome"
}
